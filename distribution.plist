<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>Tcl/Tk</title>
    <background alignment="bottomleft" file="Tcl9Logo.png" mime-type="image/png" scaling="proportional"/>
    <background-darkAqua alignment="bottomleft" file="Tcl9Logo-dark.png" mime-type="image/png" scaling="proportional"/>
    <welcome file="Welcome.html" mime-type="text/html"/>
    <options customize="allow" require-scripts="true" hostArchitectures="x86_64,arm64"/>
    <!-- Declarations of component packages defined below -->
    <pkg-ref id="org.tcl-lang.core-Tcl9.1.0"/>
    <pkg-ref id="org.tcl-lang.core-Tk9.1.0"/>
    <pkg-ref id="org.tcl-lang.core-bin9.1.0"/>    
    <pkg-ref id="org.tcl-lang.core-TclLib2.0"/>
    <pkg-ref id="org.tcl-lang.core-TkLib0.9"/>
    <pkg-ref id="org.tcl-lang.core-Thread3.0.3"/>
    <pkg-ref id="org.tcl-lang.core-Sqlite3.50.4"/>
    <pkg-ref id="org.tcl-lang.core-TclTLS2.0"/>
    <pkg-ref id="org.tcl-lang.core-TkImg2.1.0"/>
    <pkg-ref id="org.tcl-lang.core-TDBC1.1.13"/>
    <pkg-ref id="org.tcl-lang.core-tdbcsqlite1.1.13"/>
    <pkg-ref id="org.tcl-lang.core-tDOM0.9.6"/>
    <!-- Declarations of the radio buttons on the Customize pane -->
    <choices-outline>
      <line choice="org.tcl-lang.core-Tcl9.1.0"/>
      <line choice="org.tcl-lang.core-Tk9.1.0"/>
      <line choice="org.tcl-lang.core-bin9.1.0"/>
      <line choice="org.tcl-lang.core-TclLib2.0"/>
      <line choice="org.tcl-lang.core-TkLib0.9"/>
      <line choice="org.tcl-lang.core-Thread3.0.3"/>
      <line choice="org.tcl-lang.core-Sqlite3.50.4"/>
      <line choice="org.tcl-lang.core-TclTLS2.0"/>
      <line choice="org.tcl-lang.core-TkImg2.1.0"/>
      <line choice="org.tcl-lang.core-TDBC1.1.13"/>
      <line choice="org.tcl-lang.core-tdbcsqlite1.1.13"/>
      <line choice="org.tcl-lang.core-tDOM0.9.6"/>
    </choices-outline>
    <!-- The component packages associate with the buttons -->
    <choice id="org.tcl-lang.core-Tcl9.1.0" title="Tk">
        <pkg-ref id="org.tcl-lang.core-Tcl9.1.0"/>
    </choice>
    <choice id="org.tcl-lang.core-Tk9.1.0" title="Tcl">
        <pkg-ref id="org.tcl-lang.core-Tk9.1.0"/>
    </choice>
    <choice id="org.tcl-lang.core-bin9.1.0" title="/usr/local/bin">
        <pkg-ref id="org.tcl-lang.core-bin9.1.0"/>
    </choice>
    <choice id="org.tcl-lang.core-TclLib2.0" title="TclLib">
        <pkg-ref id="org.tcl-lang.core-TclLib2.0"/>
    </choice>
    <choice id="org.tcl-lang.core-TkLib0.9" title="TkLib">
        <pkg-ref id="org.tcl-lang.core-TkLib0.9"/>
    </choice>
    <choice id="org.tcl-lang.core-Thread3.0.3" title="Thread">
        <pkg-ref id="org.tcl-lang.core-Thread3.0.3"/>
    </choice>
    <choice id="org.tcl-lang.core-Sqlite3.50.4" title="Sqlite">
        <pkg-ref id="org.tcl-lang.core-Sqlite3.50.4"/>
    </choice>
    <choice id="org.tcl-lang.core-TclTLS2.0" title="TclTLS">
        <pkg-ref id="org.tcl-lang.core-TclTLS2.0"/>
    </choice>
    <choice id="org.tcl-lang.core-TkImg2.1.0" title="TkImg">
        <pkg-ref id="org.tcl-lang.core-TkImg2.1.0"/>
    </choice>
    <choice id="org.tcl-lang.core-TDBC1.1.13" title="TDBC">
        <pkg-ref id="org.tcl-lang.core-TDBC1.1.13"/>
    </choice>
    <choice id="org.tcl-lang.core-tdbcsqlite1.1.13" title="tdbc::sqlite3">
        <pkg-ref id="org.tcl-lang.core-tdbcsqlite1.1.13"/>
    </choice>
    <choice id="org.tcl-lang.core-tDOM0.9.6" title="tDOM">
        <pkg-ref id="org.tcl-lang.core-tDOM0.9.6"/>
    </choice>
    <!-- Descriptions of the component packages -->
    <!-- Apple uses the version number to decide if a package is installed -->
    <pkg-ref id="org.tcl-lang.core-Tcl9.1.0" version="9.1.0" onConclusion="none">
      tcl.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-Tk9.1.0" version="9.1.0" onConclusion="none">
      tk.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-bin9.1.0" version="9.1.0" onConclusion="none">
      bin.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-TclLib2.0" version="2.0" onConclusion="none">
      tcllib.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-TkLib0.9" version="0.9" onConclusion="none">
      tklib.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-Thread3.0.3" version="3.0.3" onConclusion="none">
      thread.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-Sqlite3.50.4" version="3.50.4" onConclusion="none">
      sqlite.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-TclTLS2.0" version="2.0" onConclusion="none">
      tcltls.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-TkImg2.1.0" version="2.1.0" onConclusion="none">
      tkimg.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-TDBC1.1.13" version="1.1.13" onConclusion="none">
      tdbc.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-tdbcsqlite1.1.13" version="1.1.13" onConclusion="none">
      tdbcsqlite3.pkg
    </pkg-ref>
    <pkg-ref id="org.tcl-lang.core-tDOM0.9.6" version="0.9.6" onConclusion="none">
      tdom.pkg
    </pkg-ref>
</installer-gui-script>
